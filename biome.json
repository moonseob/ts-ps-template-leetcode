{
    "$schema": "https://biomejs.dev/schemas/2.3.11/schema.json",
    "formatter": {
        "useEditorconfig": true,
        "formatWithErrors": true,
        "lineWidth": 120
    },
    "javascript": {
        "formatter": {
            "quoteStyle": "double"
        }
    },
    "assist": {
